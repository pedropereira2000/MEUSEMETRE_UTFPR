package Aluno2;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class AlunoTest {

	@Test
	void testCalcMedias() {
		var alu = new Aluno();
		alu.recebeNota1(6.5);
		alu.recebeNota2(7.0);
		alu.recebeNota3(8.7);
		alu.recebeNota4(10.0);
		assertEquals(8.05, alu.imprimeMedia(), 0.01);
	}

}
